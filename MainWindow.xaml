<?xml version="1.0" encoding="utf-8"?>
<Window
    x:Class="AppClientes.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AppClientes"
    xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
    >

    <muxc:NavigationView PaneDisplayMode="Left" OpenPaneLength="200">
        <muxc:NavigationView.MenuItems>
            <muxc:NavigationViewItem Content="Home" Icon="Home" Tapped="OnMenuItemTapped"/>
            <muxc:NavigationViewItem Content="Clientes" Icon="Home" />
            <muxc:NavigationViewItem Content="Proveedores" Icon="Contact"/>
            <muxc:NavigationViewItem Content="Facturas de Venta" Icon="Contact"/>
        </muxc:NavigationView.MenuItems>

        <Frame x:Name="contentFrame"/>
    </muxc:NavigationView>
</Window>
